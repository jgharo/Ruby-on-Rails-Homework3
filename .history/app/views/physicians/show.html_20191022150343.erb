<p id="notice"><%= notice %></p>

<div class="physician">
  <h2>
      <%= @physician.name %>
  </h2>
  <h3>
      Specialty:
      <%= @physician.specialty %>
  </h3>
</div>
<hr>

<h3>Office Locations</h3>
<div class="location-table">
    <table class="blueTable">
        <% @physician.offices.each do |offices| %>
          <tr>
              <th>Street Address</th>
              <th>Zip Code</th>
              <th>Phone</th>
          </tr>
          <tr>
              <td><%= offices.street_address %></td>
              <td><%= offices.zip %></td>
              <td><%= offices.phone %></td>
          </tr>
        <% end %>
        
            
